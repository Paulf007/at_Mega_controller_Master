void toggleLED (){              
              digitalWrite(13, LOW); // indicate the reset will happen
             delay(100);
             digitalWrite(13, HIGH);
             delay(100);
             digitalWrite(13, LOW);
             delay(100);
             digitalWrite(13, HIGH);
             delay(100);
             digitalWrite(13, LOW);
             delay(100);
             digitalWrite(13, HIGH);
             delay(100);
             digitalWrite(13, LOW);
             delay(100);
             digitalWrite(13, HIGH);
             delay(100);
             digitalWrite(13, LOW);
             delay(100);
             digitalWrite(13, HIGH);
             delay(100);
             digitalWrite(13, LOW);
             delay(100);
             digitalWrite(13, HIGH);
             delay(100);
             digitalWrite(13, LOW);              
  } 
void toggleLED2 (){              
              digitalWrite(13, LOW); // indicate the reset will happen
             delay(200);
             digitalWrite(13, HIGH);
             delay(200);
             digitalWrite(13, LOW);
             delay(300);
             digitalWrite(13, HIGH);
             delay(300);
             digitalWrite(13, LOW);
             delay(400);
             digitalWrite(13, HIGH);           
  }  
